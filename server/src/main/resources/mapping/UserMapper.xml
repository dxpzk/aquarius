<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.dxpzk.mapper.UserMapper">
    <cache type="com.example.dxpzk.config.MybatisRedisCache"/>
    <cache/>
    <resultMap id="BaseResultMap" type="com.example.dxpzk.model.UserModel">
        <result column="ID" property="id" jdbcType="BIGINT"/>
        <result column="NAME" property="name" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="query" resultMap="BaseResultMap" parameterType="java.lang.Integer" useCache="true">
        select * from user
        where id = #{id}
        ORDER BY id ASC limit 1
    </select>
</mapper>